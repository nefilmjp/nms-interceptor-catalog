{
  "name": "nms-interceptor-catalog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p next:dev tsm:watch",
    "next:dev": "next dev",
    "build": "next build",
    "start": "cd out && serve",
    "eslint": "eslint --cache ./**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx}",
    "eslint:fix": "eslint --cache --fix ./**/*.{js,mjs,cjs,jsx,ts,mts,cts,ts}",
    "stylelint": "stylelint ./**/*.{css,scss}",
    "stylelint:fix": "stylelint --fix ./**/*.{css,scss}",
    "prettier": "prettier --write ./**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,css,scss}",
    "next:lint": "next lint",
    "lint": "run-s next:lint eslint:fix stylelint:fix prettier type-check",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "tsm": "typed-scss-modules \"src/**/*.module.scss\"",
    "tsm:watch": "typed-scss-modules \"src/**/*.module.scss\" --watch",
    "prepare": "npx simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged -p false -c .lintstagedrc.mjs"
  },
  "volta": {
    "node": "18.17.0",
    "pnpm": "8.6.6"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "chakra-react-select": "^4.7.0",
    "chart.js": "^4.3.2",
    "chartjs-plugin-datalabels": "^2.2.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.15.0",
    "lodash-es": "^4.17.21",
    "lokijs": "^1.5.12",
    "lz-string": "^1.5.0",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-use": "^17.4.0",
    "react-virtualized": "^9.22.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/lodash-es": "^4.17.8",
    "@types/lokijs": "^1.5.8",
    "@types/node": "20.4.5",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@types/react-virtualized": "^9.21.22",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "dotenv": "^16.3.1",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-i": "2.28.0-2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.0.0",
    "sass": "^1.64.2",
    "sass-loader": "^13.3.2",
    "serve": "^14.2.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.2",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "typed-scss-modules": "^7.1.2",
    "typescript": "5.1.6",
    "webpack": "^5.88.2"
  }
}
